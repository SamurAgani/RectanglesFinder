{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DbConnectionString": "Data Source=WIN-E5INKAJM1MQ;Initial Catalog=RectangleDB;Integrated Security=True;Connect Timeout=30;Encrypt=False",
  "MasterDatabase": "Data Source=WIN-E5INKAJM1MQ;Initial Catalog=master;Integrated Security=True;Connect Timeout=30;Encrypt=False",
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Async" ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "Matching.FromSource('Microsoft')"
        }
      },
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "Matching.FromSource('System')"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Information"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo:Information": {
      "Name": "Async",
      "Args": {
        "Configure": [
          {
            "Name": "File",
            "Args": {
              "RestrictedToMinimumLevel": "Debug",
              "path": "logs/RectangleFinder-.log",
              "rollingInterval": "Day",
              "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] Thread:{ThreadId}, Message: {Message:lj}, {NewLine} {Exception}"
            }
          }
        ]
      }
    },
    "WriteTo:Error": {
      "Name": "Async",
      "Args": {
        "Configure": [
          {
            "Name": "File",
            "Args": {
              "RestrictedToMinimumLevel": "Error",
              "path": "logs/Errors-.log",
              "rollingInterval": "Day",
              "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] Thread:{ThreadId}, Message: {Message:lj}, {NewLine} {Exception}"
            }
          }
        ]
      }
    }
  }
}
